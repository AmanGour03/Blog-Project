<div class="author">
  <h1 class="author__heading">Hi, I am Aman.</h1>
  <p class="author__body">Java Programmer and Web Developer</p>
</div>

<img
  src="/img/aaron-burden-AXqMy8MSSdk-unsplash.jpg"
  alt="person looking out of window"
  class="hero-image"
  width="981"
  height="528"
/>
<section class="article">
  <h2 class="article__heading">Latest Posts</h2>
  <ul class="article-ul">
    <% data.forEach(post => { %>
      <li>
        <a href="/post/<%= post._id %>">
          <span><%= post.title %></span>
          <span class="article-list__date"><%= post.createdAt.toDateString()%></span>
        </a>
      </li>
      <% }) %>
  </ul>
  <% if (nextPage !==null) { %>
    <a href="/?page=<%= nextPage %>" class="pagination">&lt; View Older Posts</a>
   
  <% } %>
</section>
